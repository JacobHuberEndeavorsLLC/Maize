<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:MaizeUI.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Width="600" Height="300"
        WindowStartupLocation="CenterScreen"
        Background="Transparent"
        TransparencyLevelHint="AcrylicBlur"
        ExtendClientAreaToDecorationsHint="True"
        CanResize="True"
        x:Class="MaizeUI.Views.AppsettingsNoticeWindow"
        Title="Verify!">
  <Design.DataContext>
    <vm:AppsettingsNoticeWindowViewModel/>
  </Design.DataContext>

  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
    <TextBlock Foreground="#c29c09" FontSize="20" Margin="20 0 20 0" Text="{Binding Notice}" TextWrapping="Wrap"/>
    <TextBox Margin="20 0 20 0" IsReadOnly="True" Text="{Binding Location}" TextWrapping="Wrap"/>
  </StackPanel>
</Window>
